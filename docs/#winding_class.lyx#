\begin_body

\begin_layout Title
Winding class 
\end_layout

\begin_layout Standard
The SU2 field is defined as 
\begin_inset Formula $W_{i}=W_{i}^{a}\frac{\sigma^{a}}{2i}$
\end_inset

, 
\begin_inset Formula $W_{i}^{a}=\text{Tr}i\sigma^{a}W_{i}$
\end_inset

.
\end_layout

\begin_layout Standard
The covariant derivative is defined as 
\begin_inset Formula $D_{\mu}\Phi=\left(\partial_{\mu}-\frac{i}{2}g\sigma^{a}W_{i}^{a}\right)\Phi=\left(\partial_{\mu}+gW_{\mu}\right)\Phi$
\end_inset

.
\end_layout

\begin_layout Standard
The gauge transformation is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\Phi & \rightarrow U\Phi\\
D_{\mu}\Phi & \rightarrow UD_{\mu}\Phi\\
W_{\mu} & \rightarrow UW_{\mu}U^{-1}+\frac{1}{g}U\partial_{\mu}U^{-1}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
For the hedgehog choice,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U=\cos\frac{f(r)}{2}+i\sigma^{a}\hat{w^{a}}\sin\frac{f(r)}{2}=\exp\left(\frac{i}{2}\bm{\sigma}\cdot\bm{w}f(r)\right)
\]

\end_inset


\end_layout

\begin_layout Standard
and 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
U^{-1}=\cos\frac{f(r)}{2}-i\sigma^{a}\hat{w^{a}}\sin\frac{f(r)}{2}
\]

\end_inset


\end_layout

\begin_layout Standard
where, 
\begin_inset Formula $\hat{w^{a}}=(\frac{x}{r},\frac{y}{r},\frac{z}{r})$
\end_inset

; the profile function 
\begin_inset Formula $f(r)$
\end_inset

 satisfies that 
\begin_inset Formula $f(0)=0$
\end_inset

 and 
\begin_inset Formula $f(\infty)=2\pi N_{w}$
\end_inset

, 
\begin_inset Formula $N_{w}\in\mathbb{Z}$
\end_inset

.
\end_layout

\begin_layout Standard
The derivative of 
\begin_inset Formula $U$
\end_inset

 is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\partial_{i}U=-\frac{1}{2}\sin\frac{f(r)}{2}\times f'(r)\partial_{i}r+i\sigma^{a}\left[\partial_{i}\hat{w^{a}}\times\sin\frac{f(r)}{2}+\frac{1}{2}\hat{w^{a}}\cos\frac{f(r)}{2}\times f'(r)\partial_{i}r\right]
\]

\end_inset


\end_layout

\begin_layout Standard
The derivative of 
\begin_inset Formula $U^{-1}$
\end_inset

 is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\partial_{i}U^{-1}=-\frac{1}{2}\sin\frac{f(r)}{2}\times f'(r)\partial_{i}r-i\sigma^{a}\left[\partial_{i}\hat{w^{a}}\times\sin\frac{f(r)}{2}+\frac{1}{2}\hat{w^{a}}\cos\frac{f(r)}{2}\times f'(r)\partial_{i}r\right]
\]

\end_inset


\end_layout

\begin_layout Standard
We have the following results:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\partial_{i}r=\frac{x_{i}}{r}
\]

\end_inset


\end_layout

\begin_layout Standard
and 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\partial_{i}\hat{w^{a}}=\left(\begin{array}{c}
\partial_{x}\hat{w^{a}}\\
\partial_{y}\hat{w^{a}}\\
\partial_{z}\hat{w^{a}}
\end{array}\right)=\left(\begin{array}{ccc}
\frac{y^{2}+z^{2}}{r^{3}} & \frac{-xy}{r^{3}} & \frac{-xz}{r^{3}}\\
\frac{-xy}{r^{3}} & \frac{x^{2}+z^{2}}{r^{3}} & \frac{-yz}{r^{3}}\\
\frac{-xz}{r^{3}} & \frac{-yz}{r^{3}} & \frac{x^{2}+y^{2}}{r^{3}}
\end{array}\right)=\frac{1}{r}\left(\delta_{i}^{a}-\frac{x^{a}x_{i}}{r^{2}}\right).
\]

\end_inset


\end_layout

\begin_layout Standard
The pure gauge is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
gW=U\text{d}U^{-1}=\frac{1}{2i}\bm{\sigma}\cdot\left[\hat{w}\ \text{d}f+\sin f\ \text{d}\hat{w}-\left(1-\cos f\right)\left(\hat{w}\times\text{d}\hat{w}\right)\right].
\]

\end_inset


\end_layout

\begin_layout Standard
The component form is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
gW_{i}^{a} & =w^{a}\partial_{i}f+\sin f\ \partial_{i}w^{a}-(1-\cos f)\ \epsilon_{abc}w_{b}\partial_{i}w_{c}\\
 & =\frac{\sin f}{r}\delta_{i}^{a}+\left(f'-\frac{\sin f}{r}\right)\frac{x^{a}x^{i}}{r^{2}}+\frac{1-\cos f}{r^{2}}\epsilon_{aib}x^{b}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The CS number for the pure gauge field is 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{CS}=\frac{k}{4\pi}\int_{S^{3}}\text{Tr }(U\text{d}U^{-1})^{3}=-N_{w},
\]

\end_inset


\end_layout

\begin_layout Standard
where the constant is 
\begin_inset Formula $\frac{k}{4\pi}=\frac{1}{24\pi^{2}}$
\end_inset

.
\end_layout

\end_body
\end_document
